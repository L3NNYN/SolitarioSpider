<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>

<VBox fx:id="root" alignment="CENTER" spacing="20.0" stylesheets="@resources/Style.css" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="cr.ac.una.solitariospider.PrimaryController">
   <children>
      <AnchorPane fx:id="raat" prefHeight="200.0" prefWidth="200.0">
         <children>
            <Label fx:id="lblPrimary" onMouseDragged="#labeldDragg" onMouseEntered="#labelEnter" onMousePressed="#labelPress" onMouseReleased="#labelRelease" text="Label prueba" />
            <Button fx:id="primaryButton" onAction="#switchToSecondary" styleClass="btn-one" text="Carta" />
         </children>
      </AnchorPane>
   </children>
   <padding>
      <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
   </padding>
</VBox>
